{
  "name": "@benthaminstruments/ben-to-json",
  "version": "1.0.8",
  "description": "Convert Bentham .ben binary files to JSON",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/BenthamInstruments/ben-to-json.git"
  },
  "author": {
    "name": "Bentham Instruments"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "fmt": "prettier --write **/*.ts* **/*.js **/*.json*",
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu jest"
  },
  "dependencies": {
    "cfb": "^1.2.2"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "deep-map": "^2.0.0",
    "es6-weak-map": "^2.0.3",
    "full-icu": "^1.5.0",
    "jest": "^28.1.3",
    "lodash": "^4.17.21",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "verbose": true
  }
}
